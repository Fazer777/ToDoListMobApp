<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/notesFragment">
    <fragment
        android:id="@+id/notesFragment"
        android:name="com.example.todolistver2.fragments.Notes.NotesFragment"
        android:label="fragment_notes"
        tools:layout="@layout/fragment_notes" >
        <argument
            android:name="notes"
            app:argType="com.example.todolistver2.Models.Note[]" />
    </fragment>
    <fragment
        android:id="@+id/tasksFragment"
        android:name="com.example.todolistver2.fragments.Tasks.TasksFragment"
        android:label="fragment_tasks"
        tools:layout="@layout/fragment_tasks" />
    <fragment
        android:id="@+id/timerFragment"
        android:name="com.example.todolistver2.fragments.Timer.TimerFragment"
        android:label="fragment_timer"
        tools:layout="@layout/fragment_timer" />
    <fragment
        android:id="@+id/defaultFragmentNotes"
        android:name="com.example.todolistver2.fragments.DefaultFragment.DefaultFragmentNotes"
        android:label="fragment_default_notes"
        tools:layout="@layout/fragment_default_notes" />
</navigation>